{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "object-literal-sort-keys": false,
    "no-namespace": [
      true,
      "allow-declarations"
    ],
    "class-name": false,
    "no-empty-interface": false,
    "forin": false,
    "no-shadowed-variable": false,
    "interface-over-type-literal": false,
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "curly": [
      true,
      "ignore-same-line"
    ]
  },
  "jsRules": {}
}
